extends ../layout 

block content
  .container
    h2 Admin
    .spacer
    ul.nav.nav-pills.nav-justified
      li.nav-item
        a#general-tab.nav-link(href='#general', data-toggle='tab', aria-controls='general', aria-selected='true') General
      li.nav-item
        a#users-tab.nav-link(href='#users', data-toggle='tab', aria-controls='users', aria-selected='false') Users
      li.nav-item
        a#venues-tab.nav-link(href='#venues', data-toggle='tab', aria-controls='venues', aria-selected='false') Venues
    .tab-content
      #general.tab-pane.fade.active(role='tabpanel',aria-labelledby='general-tab') test 1
      #users.tab-pane.fade.active(role='tabpanel',aria-labelledby='users-tab') me two
      #venues.tab-pane.fade.active(role='tabpanel',aria-labelledby='venues-tab')
        include ../partials/admin.venues.pug
    include ../modals/venueModal
  script(src='/js/admin.js') 